	<array>
		<dict>
			<key>CFBundleURLSchemes</key>
			<array>
				<string>pixie</string>
			</array>
			<key>CFBundleURLName</key>
			<string>com.guitaripod.pixie</string>
		</dict>
	</array>
</dict>
</plist>
